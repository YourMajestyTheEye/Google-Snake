javascript: window.snake.toggle_death = function() {if(document.getElementsByClassName("wjOYOd")[0].style.opacity==0){document.getElementsByClassName("wjOYOd")[0].style="opacity:1;visibility:visible;";}else if(document.getElementsByClassName("wjOYOd")[0].style.opacity){document.getElementsByClassName("wjOYOd")[0].style="opacity:0;visibility:hidden;";};};window.snake.toggle_death();
